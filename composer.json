{
    "name": "funxtionatics/wefitter-client",
    "description": "Generated from the WeFitter documentation",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/funxtionatics/wefitter-client",
    "license": "proprietary",
    "authors": [
        {
            "name": "FunXtion",
            "email": "developers@funxtion.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "WeFitter\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "WeFitter\\Client\\" : "test/" }
    }
}
